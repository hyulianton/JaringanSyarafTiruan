    {
      "cell_type": "code",
      "source": [
        "# Dapatkan mapping indeks ke kata untuk bahasa Indonesia\n",
        "id_vocab = id_vectorizer.get_vocabulary()\n",
        "id_index_to_word = dict(zip(range(len(id_vocab)), id_vocab))\n",
        "start_token = id_vectorizer(['[START]'])[0].numpy()[0]\n",
        "end_token = id_vectorizer(['[END]'])[0].numpy()[0]\n",
        "\n",
        "def tokens_to_text(tokens):\n",
        "    words = [id_index_to_word.get(i, '') for i in tokens]\n",
        "    # Gabungkan dan bersihkan token khusus\n",
        "    text = ' '.join(words)\n",
        "    text = re.sub(r' \\[START\\] | \\[END\\] ', '', text)\n",
        "    text = re.sub(r'\\s+([?.!,])', r'\\1', text) # Bersihkan spasi sebelum tanda baca\n",
        "    return text.strip()\n",
        "\n",
        "def translate(input_sentence):\n",
        "    # 1. Preprocessing Input (Inggris)\n",
        "    input_tensor = en_vectorizer([input_sentence])\n",
        "\n",
        "    # 2. Inisialisasi Output (Indonesia) dengan token [START]\n",
        "    output_tokens = [start_token]\n",
        "\n",
        "    # 3. Proses Auto-Regressive\n",
        "    for i in range(MAX_TOKENS - 1):\n",
        "        # Bentuk input decoder: [START] token, dan token yang sudah diprediksi\n",
        "        # Sekarang, kita harus PAD urutan ke panjang 127 (MAX_TOKENS - 1)\n",
        "\n",
        "        # Buat padding\n",
        "        current_len = len(output_tokens)\n",
        "        padding_needed = (MAX_TOKENS - 1) - current_len\n",
        "\n",
        "        if padding_needed < 0:\n",
        "             # Seharusnya tidak terjadi jika MAX_TOKENS tepat\n",
        "             output_tokens = output_tokens[:MAX_TOKENS - 1]\n",
        "             padding_needed = 0\n",
        "\n",
        "        # Tambahkan padding (0) di belakang urutan\n",
        "        padded_tokens = output_tokens + [0] * padding_needed\n",
        "\n",
        "        # Bentuk tensor input: (1, 127)\n",
        "        decoder_input_tokens = tf.convert_to_tensor(\n",
        "            np.array(padded_tokens).reshape(1, -1), dtype=tf.int64\n",
        "        )\n",
        "\n",
        "        # Prediksi\n",
        "        # Model akan memprediksi 127 token; kita hanya butuh prediksi untuk token terakhir yang kita masukkan.\n",
        "        predictions = transformer([input_tensor, decoder_input_tokens])\n",
        "\n",
        "        # Ambil prediksi terakhir *yang valid* pada urutan\n",
        "        # Karena kita memprediksi 127 token, prediksi yang valid adalah prediksi pada posisi 'current_len - 1'\n",
        "        last_prediction = predictions[0, current_len - 1, :]\n",
        "\n",
        "        # Ambil token dengan probabilitas tertinggi (greedy decoding)\n",
        "        predicted_id = tf.argmax(last_prediction).numpy()\n",
        "\n",
        "        # Tambahkan token ke urutan output (bukan ke padded_tokens)\n",
        "        output_tokens.append(predicted_id)\n",
        "\n",
        "        # Hentikan jika token [END] terdeteksi atau urutan sudah penuh\n",
        "        if predicted_id == end_token or len(output_tokens) >= MAX_TOKENS:\n",
        "            break\n",
        "\n",
        "    # 4. Konversi token ke teks\n",
        "    return tokens_to_text(output_tokens)\n",
